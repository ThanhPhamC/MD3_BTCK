<!-- Product Section Begin -->
<section class="product spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>New product</h4>
                </div>
            </div>
            <div class="col-lg-8 col-md-8">
                <ul class="filter__controls">
                    <li class="active" data-filter="*">All</li>
                    <li data-filter=".women">Women’s</li>
                    <li data-filter=".men">Men’s</li>
                    <li data-filter=".kid">Kid’s</li>
                    <li data-filter=".accessories">Accessories</li>
                    <li data-filter=".cosmetic">Cosmetics</li>
                </ul>
            </div>
        </div>
        <div class="row property__gallery">
            <c:forEach items="${productList}" var="pr">
            <div class="col-lg-3 col-md-4 col-sm-6 mix women" >
                <div class="product__item">
                    <div class="product__item__pic set-bg" >
                        <a href="<%=request.getContextPath()%>/HomeServlet?action=Detail&&productId=${pr.productId}"><img src="<%=request.getContextPath()%>/imgProduct/${pr.productImg}" alt=""></a>
                        <div class="label new">New</div>
                        <ul class="product__hover">
                            <li><a href="<%=request.getContextPath()%>/imgProduct/${pr.productImg}" class="image-popup"><span class="arrow_expand"></span></a></li>
                            <li><a href="#"><span class="icon_heart_alt"></span></a></li>
                            <li><a href="#"><span class="icon_bag_alt"></span></a></li>
                        </ul>
                    </div>
                    <div class="product__item__text">
                        <h6><a href="<%=request.getContextPath()%>/HomeServlet?action=Detail&&productId=${pr.productId}">${pr.productName}</a></h6>
                        <div class="rating">${pr.starPoint}
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price"> ${pr.exportPrice*(1-pr.discount/100)} VND<span>${pr.exportPrice}</span></div>
                    </div>
                </div>
            </div>
            </c:forEach>
        </div>
    </div>
</section>
